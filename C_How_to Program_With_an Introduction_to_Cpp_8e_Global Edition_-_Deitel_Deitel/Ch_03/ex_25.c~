#include <stdio.h>

int main( void )
{

  int number, plusThree, plusSix, plusNine, counter = 0;

  puts( "A\tA^2\tA^3\tA^4" );
  
  while( counter < 5 ) {
    plusThree = number + 3;
    plusSix = plusThree + 3;
    plusNine = plusSix + 3;
    counter++;
    printf( "%d\t%d\t%d\t%d\n", number, plusThree, plusSix, plusNine );
  }
}
