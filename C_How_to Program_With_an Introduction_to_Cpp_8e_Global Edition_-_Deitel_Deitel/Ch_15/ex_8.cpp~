#include <iostream>
#include "swap.h"
using namespace std;

int main()
{
  int a, b;

  cout << "Enter two integers: " << endl;
  cin >> a >> b;

  cout << a << " * " << b << " = " << product(a, b) << endl;

  double c, d;

  cout << "Enter two doubles: " << endl;
  cin >> c >> d;

  cout << c << " * " << d << " = " << product(c, d) << endl;
}
