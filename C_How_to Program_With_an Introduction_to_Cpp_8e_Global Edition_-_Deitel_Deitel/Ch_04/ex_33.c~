#include <stdio.h>

int main(void)
{
  int decimal;
  
  puts("Enter a decimal number:");
  scanf("%d", &decimal);

  }

  for (int counter = 1; counter <= decimal; counter++) {
    if ((number % 100) % 10 == 0) {
      printf("%s", "X");
    }
    if ((number % 5) != 0) {
    printf("%s", "I");
    }
    if ((counter % 10) % 4 == 0) {
      printf("%s", "V");
