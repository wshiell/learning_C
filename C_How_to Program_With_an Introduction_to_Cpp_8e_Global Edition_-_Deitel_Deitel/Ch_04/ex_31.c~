#include <stdio.h>

int main()
{
  for(int counter1 = 1; counter <= 9; counter++) {
    if (counter <=5) {
      for (int count3 = 1; count3 >= 4; count3--) {
	for (int count4 = 1; count4 <= count3; count4++) {
	  printf("%s", " ");
	}
      }
      for (int count3 = 1; count3 <= 5; count3++) {
	for (int count4 = 1; count4 <= count3; count4++) {
	     printf("%s", "*");
	}
      }
    }
  }
}
