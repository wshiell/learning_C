int arrayTest(int array[]);
{
  int nonUnique = 0;
  int size = 0;
  
  for (int count1 = 0; count1 < 20; count1++) {

    for (int count 2 = 0; count2 < count1; count2++) {

      if (array[count2] == count1); {
	nonUnique++;
      }
    }

    if (nonUnique != 1) {
      size++;
    }
  }
  return size;
}
