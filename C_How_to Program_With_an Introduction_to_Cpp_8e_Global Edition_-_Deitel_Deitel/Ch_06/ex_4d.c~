#include <stdio.h>
#define SIZE 3

int main(void)
{
  int table[SIZE][SIZE];
 
  for (int x = 0; x < SIZE; x++) {
    for (int y = 0; y < SIZE; y++) {
      table[x][y] = x + y;
    }
  }
  
  for (int x = 0; x < SIZE; x++) {
    for (int y = 0; y < SIZE; y++) {
      printf("%d ", table[SIZE][SIZE]);
    }
    puts("");
  }
  
  int table[SIZE][SIZE] = { { 1, 8 }, { 2, 4, 6 }, { 5 } };  

  for (int x = 0; x < SIZE; x++) {
    for (int y = 0; y < SIZE; y++) {
      printf("%d ", table[SIZE][SIZE]);
    }
    puts("");
  }
}
