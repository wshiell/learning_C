#include <stdio.h>

int main(void)
{
  int fahrenheit = 0;
  double celcius;

  calsius = 5.0 / 9.0 * (fahrenheit - 32);
  
  printf("\n%s\n", "*** Fahrenheit to Celcius Temperature Converter ***");

  for (int temp = 0; temp < 212; temp++) {

    printf("%+-10.3f%c", (temp % 2) % 2 ? '\n' : '\t');
  }
  
}
