#include <stdio.h>

int main(void)
{
  float celcius, fahrenheit;

  printf("%12s, %12s", "Fahrenheit", "Celcius");  

  for ( unsigned int temp = 30; temp <= 50; temp++) {
    celcius = (float) temp;
    fahrenheit = (9 / 5) * celsius + 32;
    printf("%12.2f, %12.2f", celcius, fahrenheit);
    
  }
}
