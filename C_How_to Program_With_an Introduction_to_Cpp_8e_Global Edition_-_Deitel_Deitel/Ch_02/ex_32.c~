#include <stdio.h>

int main ( void )
{

  int height, weight, BMI;

  puts( "Enter your height (in meters): " );
  scanf( "%d", &height );

  puts( "Enter your weight (in kilograms): " );
  scanf( "%d", &weight );

  printf( "Your BMI is: \n\n%d", BMI );

  puts( "BMI VALUES" );
  puts( "Underweight:\tless than 18.5" );
  puts( "Normal:\tbetween 18.5 and 24.9" );
  puts( "Overweight:\tbetween 25 and 29.9" );
  puts( "Obese:\t30 or greater" );

}
