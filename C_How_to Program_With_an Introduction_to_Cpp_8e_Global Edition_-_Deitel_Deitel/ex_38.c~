#include <stdio.h>

int main(void)
{
  int day = 1;

  printf("%s", "On the ");
  
  switch (day) {
  case 1:
    printf("%s", "First ");
    break;
  case 2:
    printf("%s", "Second ");
    break;
  case 3:
    printf("%s", "Third ");
    break;
  case 4:
    printf("%s", "Fourth ");
    break;
  case 5:
    printf("%s", "Fifth ");
    break;
  case 6:
    printf("%s", "Sixth ");
    break;
  case 7:
    printf("%s", "Seventh ");
    break;
  case 8:
    printf("%s", "Eigth ");
    break;
  case 9:
    printf("%s", "Ninth ");
    break;
  case 10:
    printf("%s", "Tenth ");
    break;
  case 11:
    printf("%s", "Eleventh ");
    break;
  case 12:
    printf("%s", "Twelfth ");
    break;
  }

  printf("%s", "day of Christmas, my true love gave to me, ");
  
  switch (day) {
  case 12:
    printf("%s", "Twelve drummers drumming ");
  case 11:
    printf("%s", "Eleven pipers piping ");
  case 10:
    printf("%s", "Ten Lords a-leaping ");
  case 9:
    printf("%s", "Nine ladies dancing ");
  case 8:
    printf("%s", "Eight maids a-milking");
  case 7:
    printf("%s", "Seven swans a-swimming ");
  case 6:
    printf("%s", "Six gees a-laying ");
  case 5:
    printf("%s", "Five golden rings. ");
  case 4:
    printf("%s", "Four calling birds ");
  case 3:
    printf("%s", "Three French hens ");
  case 2:
    printf("%s", "Two turtle doves ");
  case 1:

    if (day > 1) {
      printf("%s", "and ");
    }
    printf("%s", "First ");
    break;
  }
}
