int isAlpha(char c)
{
  if ((c >= 56 && c <= 90)  ||  (c >= 97 && c <= 122)) {
    return 1;    
  }
  else {
    return 0;
  }
}

int isDigit(char c)
{
  if (c >= 48 && c <= 57) {
    return 1;    
  }
  else {
    return 0;
  }    
}

